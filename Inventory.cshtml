@{
   Page.title = "Inventory";
   var db = Database.Open("ETS");
var query = "SELECT * FROM parts order by parts.type";
var data = db.Query(query);
var columns = new[]{"PartId, PartDesc, PartName, Type, Quantity"};
var grid =new WebGrid(data, rowsPerPage: 50, ajaxUpdateContainerId: "grid", columnNames: columns);  
}

